{
    "name": "torow/multi-language",
    "description": "适用于laravel-admin的多语言支持扩展",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/torow/multi-language",
    "license": "MIT",
    "authors": [
        {
            "name": "wangxk",
            "email": "wangxkww@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "torow/laravel-admin": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "KevinSoft\\MultiLanguage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KevinSoft\\MultiLanguage\\MultiLanguageServiceProvider"
            ]
        }
    }
}
